<h1> Welcome to Every Trail! </h1>
<h3> Click on a trail to explore! or <%= link_to "Log out", logout_url %> </h3>
<h4> Know a trail we dont? <%= link_to "Create a trail!", new_trail_path(@trail) %> </h4>
<% @trails.each do |t| %>
    <%= link_to t.name, trail_path(t) %><br>
    <%= t.city %><br>
    <%= t.length %><br>
    <%= t.skill_level %><br>
    <%= t.description %><br>
    ------------------------
<%end%>

